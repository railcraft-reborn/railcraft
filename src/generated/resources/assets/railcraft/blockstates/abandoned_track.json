{
  "multipart": [
    {
      "when": {
        "grass": "true"
      },
      "apply": {
        "model": "minecraft:block/grass"
      }
    },
    {
      "when": {
        "shape": "north_south"
      },
      "apply": [
        {
          "model": "railcraft:block/abandoned_track_0"
        },
        {
          "model": "railcraft:block/abandoned_track_1"
        }
      ]
    },
    {
      "when": {
        "shape": "east_west"
      },
      "apply": [
        {
          "model": "railcraft:block/abandoned_track_0",
          "y": 90
        },
        {
          "model": "railcraft:block/abandoned_track_1",
          "y": 90
        }
      ]
    },
    {
      "when": {
        "shape": "ascending_east"
      },
      "apply": {
        "model": "railcraft:block/abandoned_track_raised_ne",
        "y": 90
      }
    },
    {
      "when": {
        "shape": "ascending_west"
      },
      "apply": {
        "model": "railcraft:block/abandoned_track_raised_sw",
        "y": 90
      }
    },
    {
      "when": {
        "shape": "ascending_north"
      },
      "apply": {
        "model": "railcraft:block/abandoned_track_raised_ne"
      }
    },
    {
      "when": {
        "shape": "ascending_south"
      },
      "apply": {
        "model": "railcraft:block/abandoned_track_raised_sw"
      }
    },
    {
      "when": {
        "shape": "south_east"
      },
      "apply": {
        "model": "railcraft:block/abandoned_track_corner"
      }
    },
    {
      "when": {
        "shape": "south_west"
      },
      "apply": {
        "model": "railcraft:block/abandoned_track_corner",
        "y": 90
      }
    },
    {
      "when": {
        "shape": "north_west"
      },
      "apply": {
        "model": "railcraft:block/abandoned_track_corner",
        "y": 180
      }
    },
    {
      "when": {
        "shape": "north_east"
      },
      "apply": {
        "model": "railcraft:block/abandoned_track_corner",
        "y": 270
      }
    }
  ]
}