{
  "multipart": [
    {
      "apply": {
        "model": "railcraft:block/abandoned_track_outfitted",
        "y": 0
      },
      "when": {
        "shape": "north_south"
      }
    },
    {
      "apply": {
        "model": "railcraft:block/abandoned_track_outfitted",
        "y": 90
